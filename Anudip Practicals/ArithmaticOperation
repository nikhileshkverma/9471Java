import java.util.Scanner;

class ArithmaticOperation
{
public static void main(String arg[])
{
int n1;
int n2;
int n3;
System.out.println("Enter Two Value For All Arithmatic Operation");
Scanner sc=new Scanner(System.in);
n1=sc.nextInt();
n2=sc.nextInt();  
n3=n1+n2;
System.out.println("Addition "+n3);

n3=n1-n2;
System.out.println("Substraction "+n3);

n3=n1*n2;
System.out.println("Multiplication "+n3);

n3=n1/n2;
System.out.println("Division "+n3);
}
}
